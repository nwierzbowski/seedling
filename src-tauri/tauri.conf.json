{
  "productName": "AIDME",
  "version": "0.1.0",
  "identifier": "com.seedling.dev",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "AIDME (AI Development Management Environment)",
        "width": 1400,
        "height": 900,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "focus": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
